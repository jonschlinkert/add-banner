{
  "name": "add-banner",
  "description": "Add a banner to a string. Banners are just Lo-Dash/underscore templates, if a custom one isn't defined a default will be used.",
  "version": "1.0.0",
  "homepage": "https://github.com/jonschlinkert/add-banner",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/add-banner",
  "bugs": {
    "url": "https://github.com/jonschlinkert/add-banner/issues"
  },
  "license": "MIT",
  "files": [
    "bin",
    "index.js"
  ],
  "main": "index.js",
  "bin": {
    "add-banner": "bin/add-banner.js"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
